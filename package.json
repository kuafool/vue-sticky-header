{
  "name": "vue-sticky-header",
  "version": "0.1.0",
  "main": "dist/vue-sticky-header.umd.js",
  "module": "dist/vue-sticky-header.esm.js",
  "unpkg": "dist/vue-sticky-header.min.js",
  "browser": {
    "./sfc": "src/vue-sticky-header.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-sticky-header.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-sticky-header.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-sticky-header.min.js"
  },
  "devDependencies": {
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunchaserkuafu/vue-sticky-header.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sunchaserkuafu/vue-sticky-header/issues"
  },
  "homepage": "https://github.com/sunchaserkuafu/vue-sticky-header#readme"
}
